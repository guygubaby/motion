{
  "name": "@bryce-loskie/motion",
  "version": "0.0.3",
  "description": "Opinionated motion util for the web, based on svelte animation system",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "funding": "https://github.com/sponsors/guygubaby",
  "author": "guygubaby <guygubaby@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guygubaby/motion/issues"
  },
  "homepage": "https://github.com/guygubaby/motion#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guygubaby/motion.git"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "pnpm build -- --watch",
    "build": "tsup",
    "publish-pkg": "npm publish --access public",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [],
  "dependencies": {
    "vue-demi": "latest"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.4.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
