{
  "name": "motion-monorepo",
  "version": "0.0.2",
  "description": "motion-monorepo",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "pnpm -r --parallel --filter !playground run dev",
    "build": "pnpm -r --parallel --filter !playground run build",
    "lint": "eslint \"**/*.{ts,vue,mjs}\"",
    "lint:fix": "pnpm lint -- --fix",
    "play": "pnpm -C playground run dev",
    "build:play": "pnpm -C playground run build",
    "publish-pkg": "pnpm -C packages/motion publish-pkg -- --access public",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag",
    "test": "jest",
    "test:update": "jest -u"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@bryce-loskie/eslint-config": "^1.0.6",
    "@bryce-loskie/utils": "^0.1.21",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.20",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vue/reactivity": "^3.2.31",
    "bumpp": "^7.1.1",
    "eslint": "^8.9.0",
    "eslint-plugin-jest": "^26.1.1",
    "esno": "^0.14.1",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "svelte": "^3.46.4",
    "ts-jest": "^27.1.3",
    "tsconfig-paths": "^3.12.0",
    "tsup": "^5.11.13",
    "typescript": "^4.5.5",
    "unocss": "^0.26.2",
    "vite": "^2.8.4",
    "vite-plugin-pages": "^0.21.0",
    "vue": "^3.2.31",
    "vue-router": "^4.0.13"
  }
}
